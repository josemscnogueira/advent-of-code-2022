    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "08: tree-house",
            "cargo": {
                "args": [
                    "build",
                    "--bin=tree-house",
                    "--package=advent-of-code-2022"
                ],
                "filter": {
                    "name": "tree-house",
                    "kind": "bin"
                }
            },
            "args": ["08/data/example.txt"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "09: rope-bridge",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rope-bridge",
                    "--package=advent-of-code-2022"
                ],
                "filter": {
                    "name": "rope-bridge",
                    "kind": "bin"
                }
            },
            "args": ["09/data/example.txt"],
            "cwd": "${workspaceFolder}"
        },
    ]

}
